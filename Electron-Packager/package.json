{
  "name": "electron-app",
  "version": "1.0.0",
  "description": "Electron packager ",
  "main": "main.js",
  "scripts": {
    "start": "electron .", 
    "package-win": "electron-packager . --overwrite --asar --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron App\"",
    "package-mac": "electron-packager . --overwrite --asar --platform=darwin --arch=x64 --prune=true --out=release-builds"
  },
  "author": "pedmar",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^5.0.0"
  }
}
